{
    "name": "ixocreate/cache-package",
    "description": "IXOCREATE Cache Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ixocreate/application": "dev-develop",
        "ixocreate/cache": "dev-develop"
    },
    "require-dev": {
        "ext-pdo": "*",
        "doctrine/dbal": "^2.9",
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/database-package": "dev-develop",
        "ixocreate/test": "^0.1"
    },
    "suggest": {
        "ixocreate/database-package": "Allows to use database as cache storage",
        "ext-memcached": "Allows to use memcached as cache storage"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Cache\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
