{
    "name": "ixocreate/cache-package",
    "description": "IXOCREATE Cache Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "require": {
        "php": "^7.1",
        "ixocreate/application": "^0.1",
        "ixocreate/cache": "^0.1"
    },
    "require-dev": {
        "ext-pdo": "*",
        "doctrine/dbal": "^2.9",
        "ixocreate/coding-standard": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Cache\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
