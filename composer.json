{
  "name": "ixocreate/cache-package",
  "description": "Caching package for IXOCREATE",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "IXOCREATE"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "ixocreate/cache": "dev-develop",
    "ixocreate/config": "dev-develop",
    "ixocreate/contract": "dev-develop",
    "ixocreate/servicemanager": "dev-develop"
  },
  "require-dev": {
    "ext-pdo": "*",
    "doctrine/dbal": "^2.9",
    "ixocreate/coding-standard": "^1.0",
    "phpunit/phpunit": "^6.0.6",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Ixocreate\\Package\\Cache\\": "src/"
    }
  },
  "scripts": {
    "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
  }
}
